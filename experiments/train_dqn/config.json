{
    "env": "battlesnake-9x9-easy-v0",
    "episodes": 100000,
    "render_episode_interval": 10000,
    "max_episode_length": 1000,
    "frame_stack": 2,
    "num_envs": 8,
    "training_interval": 4,

    "replay_memory": {
        "capacity": 100000,
        "epsilon": 0.01,
        "alpha": 0.7,
        "max_priority": 1.0
    },

    "dqn": {
        "batch_size": 32,
        "hidden_dim": 256,

        "learning_rate": 0.00025,
        "gamma": 0.9,
        "target_update_rate": 0.001,

        "dueling": true,
        "double": true,

        "beta": 0.4
    },

    "exploration": {
        "epsilon_min": 0.01,
        "epsilon_max": 1.0,
        "epsilon_decay": 0.9999
    }
}